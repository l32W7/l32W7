   Title = game.Name,
   Content = "Made By l32W7",
   Duration = 5,
   Image = 17362182263,
   Actions = { -- Notification Buttons
      Ignore = {
         Name = "close",
         Callback = function()
         print("The user tapped Okay!")
      end
   },
},
})



local Window = Rayfield:CreateWindow({
   Name = "Brawl 2",
   LoadingTitle = "Rellix",
   LoadingSubtitle = "by l32W7",
   ConfigurationSaving = {
      Enabled = true,
      FolderName = nil,
