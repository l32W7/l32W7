      Subtitle = "Key System",
      Note = "No method of obtaining the key is provided",
      FileName = "Key", 
      SaveKey = true, 
      GrabKeyFromSite = false, 
      Key = {"Hello"} 
   }
})



local Tab = Window:CreateTab("AutosðŸ¤–", nil) 
local CheatTab = Window:CreateTab("CheatsðŸ˜¡", nil) 
local FunTab = Window:CreateTab("FunðŸ¤­", nil) 
local SettingsTab = Window:CreateTab("SettingsðŸ”§", nil) 

local Section = Tab:CreateSection("Automatics")
local SectionCheat = CheatTab:CreateSection("Cheat")
local SectionFun = FunTab:CreateSection("Fun")
local SectionFun = SettingsTab:CreateSection("Credits!")

local Button = Tab:CreateButton({
   Name = "Auto Hit",
   Callback = function()
   while wait() do
game:GetService("ReplicatedStorage").remotes.attack:FireServer()
        end
   end,
})


local Button = Tab:CreateButton({
   Name = "Auto Block",
   Callback = function()
    if game.Players.LocalPlayer.Character.blocking.Value == true then
     game.Players.LocalPlayer.Character.blocking.Value = false
            else
     game.Players.LocalPlayer.Character.blocking.Value = true
			end
   end,
})


local Button = CheatTab:CreateButton({
   Name = "Anti Fall",
   Callback = function()  
  game:GetService("Players").LocalPlayer.Character.flailing:Destroy()           
                
                
   end,
})


local Button = CheatTab:CreateButton({
   Name = "Anti Block",
   Callback = function()     
   game:GetService("Players").LocalPlayer.Character.blocking:Destroy()       
                
                
   end,
})


local Button = CheatTab:CreateButton({
   Name = "Boost Hitbox",
   Callback = function()
   local players = game.Players:GetPlayers()
		local speaker = game.Players.LocalPlayer
		game:GetService("RunService").Stepped:Connect(function()
			for i,v in pairs(players) do
				if v ~= speaker then
					if v.Character and v.Character:FindFirstChild('HumanoidRootPart') then
						local sizeArg = 10
						local Size = Vector3.new(sizeArg,sizeArg,sizeArg)
						local Root = v.Character:FindFirstChild('HumanoidRootPart')
						if Root:IsA("BasePart") then
							if not sizeArg == 1 then
								Root.Size = Vector3.new(2,1,1)
								Root.Transparency = 0.2                
							else
								Root.Size = Size
								Root.Transparency = 0.2
							end
						end
					end
				end
			end
		end)        
   end,
})


local Keybind = FunTab:CreateKeybind({
   Name = "Fall (Only Works If You Didnt Click On The Anti Fall)",
   CurrentKeybind = "Q",
   HoldToInteract = false,
   Flag = "Keybind1", 
   Callback = function(Keybind)
   if  game.Players.LocalPlayer.Character.flailing.Value == true then
        game.Players.LocalPlayer.Character.flailing.Value = false
                else
        game.Players.LocalPlayer.Character.flailing.Value = true
 		end
  end,
})

